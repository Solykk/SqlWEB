<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="ua.com.juja.sqlweb.service"/>
    <context:component-scan base-package="ua.com.juja.sqlweb.model"/>

    <bean class="ua.com.juja.sqlweb.control.CommandsList" id="commandsList">
        <constructor-arg>
            <array>
                <bean class="ua.com.juja.sqlweb.control.commands.Connect" />
                <bean class="ua.com.juja.sqlweb.control.commands.Tables" />
                <bean class="ua.com.juja.sqlweb.control.commands.Columns" />
                <bean class="ua.com.juja.sqlweb.control.commands.TableType" />
                <bean class="ua.com.juja.sqlweb.control.commands.ColumnType" />
                <bean class="ua.com.juja.sqlweb.control.commands.Find" />
                <bean class="ua.com.juja.sqlweb.control.commands.FindSettings" />
                <bean class="ua.com.juja.sqlweb.control.commands.Clear" />
                <bean class="ua.com.juja.sqlweb.control.commands.Create" />
                <bean class="ua.com.juja.sqlweb.control.commands.Delete" />
                <bean class="ua.com.juja.sqlweb.control.commands.Drop" />
                <bean class="ua.com.juja.sqlweb.control.commands.Insert" />
                <bean class="ua.com.juja.sqlweb.control.commands.Update" />
                <bean class="ua.com.juja.sqlweb.control.commands.ReadQuery" />
                <bean class="ua.com.juja.sqlweb.control.commands.CudQuery" />
            </array>
        </constructor-arg>
    </bean>
</beans>